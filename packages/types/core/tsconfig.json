{
  "compilerOptions": {
    "moduleResolution": "node",
    "module": "esnext",
    "target": "es6",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "sourceMap": true,
    "stripInternal": true,
    "declaration": true,
    "declarationDir": ".dist/types",
    "resolveJsonModule": true,
    "plugins": []
  }
}
